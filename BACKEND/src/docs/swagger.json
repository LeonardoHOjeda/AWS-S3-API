    },
    "/tenants/{tenantUUID}": {
      "get": {
        "tags": [
          "Tenant"
        ],
        "summary": "Obtener un tenant por su UUID",
        "description": "Obtener un tenant por su UUID guardado en la base de datos",
        "operationId": "getTenantByUUID",
        "parameters": [
          {
            "name": "tenantUUID",
            "in": "path",
            "description": "UUID del tenant que se quiere obtener",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "example": {
                  "id": 1,
                  "uuid": "oo58dJPPYbhRAEj7mxtXCo",
                  "nombre": "Nomifacil_014",
                  "presignedURLTime": 3000
                },
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "integer"
                    },
                    "uuid": {
                      "type": "string"
                    },
                    "nombre": {
                      "type": "string"
                    },
                    "presignedURLTime": {
                      "type": "integer"
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Tenant no encontrado",
            "content": {
              "application/json": {
                "example": {
                  "statusCode": 404,
                  "message": "Tenant no encontrado (AWS)"
                },
                "schema": {
                  "type": "object",
                  "properties": {
                    "statusCode": {
                      "type": "integer"
                    },
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "put": {
